config global automount
        option from_fstab 0
        option anon_mount 0

config global autoswap
        option from_fstab 1
        option anon_swap 0

config mount
        option target   /dos
        option label    dos
        option fstype   vfat
        option options  rw,sync
        option enabled  1
        option enabled_fsck 0

config mount
        option target   /serval
        option label    serval
        option fstype   ext4
        option options  rw,sync
        option enabled  1
        option enabled_fsck 0

config mount
        option target   /serval-var
        option label    serval-var
        option fstype   ext4
        option options  rw,sync
        option enabled  1
        option enabled_fsck 0


